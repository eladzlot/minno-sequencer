{
    "rules": {
        "indent": [1, 4, {"SwitchCase": 1}],
        "quotes": [1,"single"],
        "linebreak-style": [2,"unix"],
        "semi": [1,"always"],
        "no-console": [1]
    },
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "requirejs" : true
    },
    "overrides": [
        {
            "files": ["**/*_test.js"],
            "env": {
                "jasmine": true
            },
            "rules": {
                "globals": {
                    "module": true,
                    "inject": true
                }
            }
        }
    ],
    "extends": "eslint:recommended"
}
